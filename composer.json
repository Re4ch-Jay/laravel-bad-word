{
    "name": "patoui/laravel-bad-word",
    "description": "Laravel bad word validation rule",
    "keywords": [
        "patoui",
        "laravel",
        "validation",
        "bad",
        "word"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrique Ouimet",
            "email": "patrique.ouimet@gmail.com",
            "homepage": "https://patriqueouimet.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/validation": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Patoui\\LaravelBadWord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patoui\\LaravelBadWord\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patoui\\LaravelBadWord\\BadWordServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
